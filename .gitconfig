[user]
  name = lgick
  email = lgick@yandex.ru
[merge]
  tool = vimdiff
[color]
  ui = true
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  push = push --follow-tags
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short --all
  type = cat-file -t
  dump = cat-file -p
  lg1 = log --pretty=format:'%h : %s' --graph
  lg = !"git lg1"
[core]
  editor = vim
	excludesFile = ~/.gitignore_global
[push]
  default = matching
